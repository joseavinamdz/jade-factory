<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.canvas.Canvas?>

<VBox fx:controller="factory.visualization.gui.SceneController" xmlns:fx="http://javafx.com/fxml">
	<children>
		<ToolBar>
			<items>
				<Button text="Add youBot" onAction="#addYouBot"/>
				<Button text="Add pressfitting" onAction="#addPressfittingStation"/>
				<Button text="Add screwing" onAction="#addScrewingStation"/>
				<Button text="Add soldering" onAction="#addSolderingStation"/>
			</items>
		</ToolBar>
		<Canvas fx:id="canvas" width="500.0" height="500.0"/>
	</children>
</VBox>